<map id="map" longitude="{{lng}}" polyline="{{polyline}}" latitude="{{lat}}" markers="{{markers}}" show-location style="width: 100%; height: 100%;">
  <cover-view class="mapTipPart" bindtap="openMapChoose">
    <cover-image class="guide_sign" src="../../assets/images/map/guide_sign.png"></cover-image>
    <cover-view class="mapTip" bindtap="openMapChoose">
      <cover-view class="_bt">选择回收箱</cover-view>
      <cover-image class="box_sign" src="../../assets/images/map/box_sign.png"></cover-image>
      <cover-view class="_no">NO0984874001</cover-view>
      <cover-view class="_name cutWord">辽宁路街道</cover-view>
      <cover-view class="_ad cutWord">宁夏路社区，宁夏路60号，小区物业旁边</cover-view>
      <cover-image class="go_route" src="../../assets/images/map/go_route.png"></cover-image>
    </cover-view>
  </cover-view>
</map>